XTerm*termName:         xterm-256color

Xcursor.theme: DMZ
Xcursor.size: 16

xscreensaver*timeout: 5
xscrensaver*lock: true

! URxvt
URxvt*loginShell: true
!urxvt color scheme:
URxvt*background: #2B2B2B
URxvt*foreground: #DEDEDE

URxvt*transparent: true
URxvt*shading:     20

URxvt*colorUL: #86a2b0
URxvt*cursorColor: #859900

! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
URxvt*color4  : #3465A4
URxvt*color12 : #729FCF
! magenta
URxvt*color5  : #75507B
URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC

!urxvt fonts: 

URxvt*font: xft:Monospace:pixelsize=13
URxvt*boldFont: xft:Monospace:bold:pixelsize=13


!urxvt extensions:

URxvt.perl-ext-common:  default,matcher,tabbed
! URxvt.perl-ext-common:  default,tabbed,matcher

!enabling clickable links:

URxvt.urlLauncher:      /usr/bin/chromium
URxvt.matcher.button:   1 

!urxvt scrolling options and cursor style:

URxvt*saveLines: 12000
!rxvt,plain, xterm, next
URxvt*scrollstyle: plain
URxvt*scrollBar: false
URxvt*cursorBlink: true
URxvt*cursorUnderline: false

! urxvt tabs settings:

URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 2
URxvt.tabbed.tab-bg: 8
URxvt.tabbed.autohide:  yes
URxvt.tabbed.new-button:  no
URxvt.tabbed.title: no

